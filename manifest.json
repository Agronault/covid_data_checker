{
    "manifest_version": 2,
    "name": "COVID Data Checker",
    "version": "0.1",
    "description": "Automatically checks the accuracy of coronavirus data on the sites you visit.",
    "browser_action": {
        "default_icon": "icons/ok.png"
      },
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["src/jquery-min.js", "src/content.js"]
        }
      ]
}